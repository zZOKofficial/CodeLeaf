<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear_background"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:paddingLeft="8dp"
		android:paddingTop="8dp"
		android:paddingRight="8dp"
		android:orientation="vertical">
		<TextView
			android:id="@+id/codeleaf_text"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingTop="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="@string/app_name"
			android:textSize="30sp"
			android:textColor="#09A97D" />
		<TextView
			android:id="@+id/catalog"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingBottom="16dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="ক্যাটালগ"
			android:textSize="25sp"
			android:textColor="?colorOnSurface" />
		<HorizontalScrollView
			android:id="@+id/hscroll2"
			android:layout_width="match_parent"
			android:layout_height="match_parent">
			<LinearLayout
				android:id="@+id/linear_scroll"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:gravity="center_horizontal|top"
				android:orientation="vertical"
				android:layout_gravity="center_horizontal|center_vertical">
				<LinearLayout
					android:id="@+id/linear_container_html_css"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal"
					android:layout_gravity="center_horizontal|center_vertical">
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview1"
						android:layout_width="175dp"
						android:layout_height="wrap_content"
						android:layout_margin="8dp"
						app:cardBackgroundColor="?colorSurfaceVariant"
						app:cardElevation="4dp"
						app:cardCornerRadius="20dp">
						<LinearLayout
							android:id="@+id/linear_html"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview1"
								android:layout_width="175dp"
								android:layout_height="175dp"
								android:src="@drawable/web_development_icon"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/html_title"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:paddingLeft="16dp"
								android:paddingTop="8dp"
								android:paddingRight="16dp"
								android:text="এইচ টি এম এল"
								android:textSize="16sp"
								android:textColor="?colorOnSurface" />
							<TextView
								android:id="@+id/html_subtitle"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="16dp"
								android:paddingRight="16dp"
								android:paddingBottom="16dp"
								android:text="বিগিনার"
								android:textSize="14sp"
								android:textColor="?colorSecondary" />
						</LinearLayout>
					</androidx.cardview.widget.CardView>
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview2"
						android:layout_width="175dp"
						android:layout_height="wrap_content"
						android:layout_margin="8dp"
						app:cardBackgroundColor="?colorSurfaceVariant"
						app:cardElevation="4dp"
						app:cardCornerRadius="20dp">
						<LinearLayout
							android:id="@+id/linear_css"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview4"
								android:layout_width="175dp"
								android:layout_height="175dp"
								android:src="@drawable/web_development_icon"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/css_title"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:paddingLeft="16dp"
								android:paddingTop="8dp"
								android:paddingRight="16dp"
								android:text="সি এস এস"
								android:textSize="16sp"
								android:textColor="?colorOnSurface" />
							<TextView
								android:id="@+id/css_subtitle"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="16dp"
								android:paddingRight="16dp"
								android:paddingBottom="16dp"
								android:text="বিগিনার"
								android:textSize="14sp"
								android:textColor="?colorSecondary" />
						</LinearLayout>
					</androidx.cardview.widget.CardView>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal"
					android:layout_gravity="center_horizontal|center_vertical">
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview3"
						android:layout_width="175dp"
						android:layout_height="wrap_content"
						android:layout_margin="8dp"
						app:cardBackgroundColor="?colorSurfaceVariant"
						app:cardElevation="4dp"
						app:cardCornerRadius="20dp">
						<LinearLayout
							android:id="@+id/linear_php"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview5"
								android:layout_width="175dp"
								android:layout_height="175dp"
								android:src="@drawable/web_development_icon"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/php_title"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:paddingLeft="16dp"
								android:paddingTop="8dp"
								android:paddingRight="16dp"
								android:text="পি এইচ পি"
								android:textSize="16sp"
								android:textColor="?colorOnSurface" />
							<TextView
								android:id="@+id/php_subtitle"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="16dp"
								android:paddingRight="16dp"
								android:paddingBottom="16dp"
								android:text="ইন্টারমিডিয়েট"
								android:textSize="14sp"
								android:textColor="?colorTertiary" />
						</LinearLayout>
					</androidx.cardview.widget.CardView>
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview4"
						android:layout_width="175dp"
						android:layout_height="wrap_content"
						android:layout_margin="8dp"
						app:cardBackgroundColor="?colorSurfaceVariant"
						app:cardElevation="4dp"
						app:cardCornerRadius="20dp">
						<LinearLayout
							android:id="@+id/linear_sql"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview6"
								android:layout_width="175dp"
								android:layout_height="175dp"
								android:src="@drawable/web_development_icon"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/sql_title"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:paddingLeft="16dp"
								android:paddingTop="8dp"
								android:paddingRight="16dp"
								android:text="সিকুয়েল"
								android:textSize="16sp"
								android:textColor="?colorOnSurface" />
							<TextView
								android:id="@+id/sql_subtitle"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="16dp"
								android:paddingRight="16dp"
								android:paddingBottom="16dp"
								android:text="ইন্টারমিডিয়েট"
								android:textSize="14sp"
								android:textColor="?colorTertiary" />
						</LinearLayout>
					</androidx.cardview.widget.CardView>
				</LinearLayout>
			</LinearLayout>
		</HorizontalScrollView>
	</LinearLayout>
</LinearLayout>
